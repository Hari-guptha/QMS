{
  "name": "qms-backend",
  "version": "1.0.0",
  "description": "Universal Queue Management System Backend",
  "author": "",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "create-admin": "ts-node -r dotenv/config scripts/create-admin.ts",
    "add-simulator-data": "ts-node -r dotenv/config mastertester/add-simulator-data.ts",
    "remove-simulator-data": "ts-node -r dotenv/config mastertester/remove-simulator-data.ts",
    "check-ticket-statuses": "ts-node -r dotenv/config check-ticket-statuses.ts"
  },
  "dependencies": {
    "@nestjs/common": "^11.1.11",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.11",
    "@nestjs/jwt": "^11.0.2",
    "@nestjs/passport": "^11.0.3",
    "@nestjs/platform-express": "^11.1.11",
    "@nestjs/platform-socket.io": "^11.1.11",
    "@nestjs/swagger": "^11.2.3",
    "@nestjs/websockets": "^11.1.11",
    "@prisma/client": "^5.22.0",
    "@types/passport-microsoft": "^2.1.1",
    "bcrypt": "^6.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "exceljs": "^4.4.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "passport-microsoft": "^2.1.0",
    "pdfmake": "^0.2.7",
    "redis": "^4.6.11",
    "reflect-metadata": "^0.1.13",
    "resend": "^2.1.0",
    "nodemailer": "^7.0.12",
    "rxjs": "^7.8.1",
    "socket.io": "^4.6.1",
    "socket.io-redis": "^6.1.1",
    "twilio": "^4.19.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.14",
    "@nestjs/schematics": "^10.0.3",
    "@nestjs/testing": "^11.1.11",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/mssql": "^4.3.1",
    "@types/node": "^20.10.6",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.35",
    "@types/pdfmake": "^0.2.4",
    "@types/pg": "^8.10.9",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "dotenv": "^17.2.3",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.2",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "prisma": "^5.22.0",
    "@types/nodemailer": "^6.4.7",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}